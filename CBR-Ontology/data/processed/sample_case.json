{
  "id": "10.1088/1742-6596/2450/1/012006",
  "year": 2023,
  "title": "RUL prediction for centrifugal pumps using CNN-LSTM on vibration",
  "venue": "Reliability Eng. & System Safety",
  "doi": "10.1088/1742-6596/2450/1/012006",
  "authors": ["Doe, J.", "Doe, Ja."],
  "case_problem": {
    "domain": "Oil&Gas",
    "asset": "centrifugal_pump",
    "failure_mode": "cavitation",
    "pdm_function": "RUL_prognosis",
    "constraints": {"latency_ms": 150},
    "assumptions": ["steady-state operation"]
  },
  "data_inputs": {
    "sensors": ["vibration", "pressure"],
    "sampling_hz": 20480,
    "features": "time_frequency",
    "dataset": "proprietary"
  },
  "case_solution": {
    "approach": "DL",
    "method": "CNN-LSTM",
    "configuration": {"window": 2048, "stride": 512},
    "explainability": "SHAP"
  },
  "evaluation": {
    "metrics": {"MAE_RUL": 3.7, "RMSE_RUL": 5.2},
    "validation": "5-fold",
    "baselines": ["LSTM", "RF"]
  },
  "outcomes": {
    "downtime_reduction_pct": 18.0,
    "notes": "Near-real-time capable on edge GPU"
  },
  "reported_in": {
    "type": "paper",
    "source_path": "data/raw/papers/pump_rul_2023.pdf",
    "open_access": true
  },
